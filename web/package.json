    "gatsby-plugin-sass": "^4.1.0",
    "gatsby-plugin-sharp": "^3.1.0",
    "gatsby-source-filesystem": "^3.1.0",
    "gatsby-source-sanity": "^7.0.0",
    "gatsby-transformer-sharp": "^3.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.32.8",
    "tsconfig-paths-webpack-plugin": "3.3.0"
